{
  "name": "vue-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "postbuild": "rm dist/mockServiceWorker.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:screenshot": "SCREEN_SHOT=true test-storybook",
    "test:unit": "vitest",
    "test:story": "test-storybook",
    "test:story2": "vitest --project=storybook",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "format": "prettier --write src/",
    "gen:doc": "typedoc --plugin typedoc-plugin-markdown src/app/index.ts",
    "gen:schema": "openapi-typescript -t ./openapi.yaml -o ./src/shared/api/v1.schema.ts --properties-required-by-default",
    "postgen:schema": "prettier --write ./src/shared/api/v1.schema.ts",
    "prepare": "husky",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "lint-staged": {
    "*.{vue,ts}": "pnpm format"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.15.0",
    "@vercel/edge": "^1.2.1",
    "@vuepic/vue-datepicker": "^10.0.0",
    "basic-auth": "^2.0.1",
    "date-fns": "^4.1.0",
    "openapi-fetch": "^0.13.4",
    "static-auth": "^2.1.2",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@playwright/test": "^1.49.1",
    "@redocly/cli": "^1.27.2",
    "@rushstack/eslint-patch": "^1.10.5",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/addon-links": "^8.5.0",
    "@storybook/addon-mdx-gfm": "^8.5.0",
    "@storybook/blocks": "^8.5.0",
    "@storybook/experimental-addon-test": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/vue3": "^8.5.0",
    "@storybook/vue3-vite": "^8.5.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/browser": "^2.1.8",
    "@vitest/eslint-plugin": "^1.1.25",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.6.0",
    "eslint": "^9.18.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-prefer-arrow-functions": "^3.6.2",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "happy-dom": "^15.11.7",
    "husky": "^9.1.7",
    "install": "^0.13.0",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.1",
    "msw": "^2.7.0",
    "msw-storybook-addon": "^2.0.4",
    "npm-run-all2": "^7.0.2",
    "openapi-msw": "^0.7.1",
    "openapi-typescript": "^7.5.2",
    "playwright": "^1.49.1",
    "prettier": "^3.4.2",
    "puppeteer": "^24.1.0",
    "reg-keygen-git-hash-plugin": "^0.14.3",
    "reg-notify-github-plugin": "^0.14.4",
    "reg-suit": "^0.14.4",
    "storybook": "^8.5.0",
    "storycap": "^5.0.1",
    "storycap-testrun": "^0.0.2",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3",
    "vite": "^5.4.11",
    "vitepress": "^1.6.3",
    "vitepress-openapi": "0.0.3-alpha.59",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.0"
  },
  "volta": {
    "node": "22.11.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}