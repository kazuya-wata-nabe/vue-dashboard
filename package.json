{
  "name": "vue-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "postbuild": "rm dist/mockServiceWorker.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:screenshot": "SCREEN_SHOT=true test-storybook",
    "test:unit": "vitest",
    "test:story": "test-storybook",
    "test:story2": "vitest --project=storybook",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "format": "prettier --write src/",
    "gen:doc": "typedoc --plugin typedoc-plugin-markdown src/app/index.ts",
    "gen:schema": "openapi-typescript -t ./openapi.yaml -o ./src/shared/api/v1.schema.ts --properties-required-by-default",
    "postgen:schema": "prettier --write ./src/shared/api/v1.schema.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{vue,ts}": "pnpm format"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.14.7",
    "@vuepic/vue-datepicker": "^8.8.1",
    "date-fns": "^3.6.0",
    "openapi-fetch": "^0.9.8",
    "vee-validate": "^4.14.7",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@playwright/test": "^1.49.0",
    "@redocly/cli": "^1.25.13",
    "@rushstack/eslint-patch": "^1.10.4",
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.4.5",
    "@storybook/addon-links": "^8.4.5",
    "@storybook/addon-mdx-gfm": "^8.4.5",
    "@storybook/blocks": "^8.4.5",
    "@storybook/experimental-addon-test": "^8.4.5",
    "@storybook/test": "^8.4.5",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/vue3": "^8.4.5",
    "@storybook/vue3-vite": "^8.4.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.7",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitest/browser": "^2.1.5",
    "@vitest/eslint-plugin": "^1.1.10",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.15.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-prefer-arrow-functions": "^3.4.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "eslint-plugin-vue": "^9.31.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "happy-dom": "^14.12.3",
    "husky": "^9.1.7",
    "install": "^0.13.0",
    "lint-staged": "^15.2.10",
    "msw": "^2.6.6",
    "msw-storybook-addon": "^2.0.4",
    "npm-run-all2": "^6.2.6",
    "openapi-msw": "^0.6.1",
    "openapi-typescript": "^7.4.3",
    "playwright": "^1.49.0",
    "prettier": "^3.3.3",
    "storybook": "^8.4.5",
    "storycap-testrun": "^0.0.2",
    "typedoc": "^0.26.11",
    "typedoc-plugin-markdown": "^4.2.10",
    "typedoc-plugin-zod": "^1.3.0",
    "typescript": "~5.4.5",
    "vite": "^5.4.11",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  },
  "volta": {
    "node": "22.11.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "packageManager": "pnpm@9.14.0"
}