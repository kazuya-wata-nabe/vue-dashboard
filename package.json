{
  "name": "vue-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "postbuild": "rm dist/mockServiceWorker.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test:unit": "vitest",
    "test:story": "test-storybook",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.ts --ignore-path .gitignore",
    "format": "prettier --write src/",
    "gen:schema": "openapi-typescript -t ./openapi.yaml -o ./src/shared/api/v1.schema.ts",
    "postgen:schema": "prettier --write ./src/shared/api/v1.schema.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{vue,ts}": "pnpm format"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.12.8",
    "@vueform/multiselect": "^2.6.7",
    "@vuepic/vue-datepicker": "^8.7.0",
    "date-fns": "^3.6.0",
    "neverthrow": "^6.2.1",
    "openapi-fetch": "^0.9.7",
    "vee-validate": "^4.12.8",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vuetify": "^3.6.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@playwright/test": "^1.44.1",
    "@rushstack/eslint-patch": "^1.8.0",
    "@storybook/addon-essentials": "^8.1.2",
    "@storybook/addon-interactions": "^8.1.2",
    "@storybook/addon-links": "^8.1.2",
    "@storybook/blocks": "^8.1.2",
    "@storybook/test": "^8.1.2",
    "@storybook/test-runner": "^0.18.2",
    "@storybook/vue3": "^8.1.2",
    "@storybook/vue3-vite": "^8.1.2",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/vue": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.5",
    "@typespec/compiler": "^0.56.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-neverthrow": "^1.1.4",
    "eslint-plugin-no-relative-import-paths": "^1.5.4",
    "eslint-plugin-prefer-arrow-functions": "^3.3.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "eslint-plugin-vue": "^9.23.0",
    "happy-dom": "^14.12.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "msw": "^2.3.0",
    "msw-storybook-addon": "^2.0.2",
    "npm-run-all2": "^6.1.2",
    "openapi-msw": "^0.6.0",
    "openapi-typescript": "^6.7.6",
    "playwright": "^1.44.1",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "storybook": "^8.1.2",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
