{
  "name": "vue-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "postbuild": "rm dist/mockServiceWorker.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test:unit": "vitest",
    "test:story": "test-storybook",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.ts --ignore-path .gitignore",
    "format": "prettier --write src/",
    "gen:schema": "openapi-typescript -t ./openapi.yaml -o ./src/shared/api/v1.schema.ts --properties-required-by-default",
    "postgen:schema": "prettier --write ./src/shared/api/v1.schema.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{vue,ts}": "pnpm format"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.13.2",
    "@vuepic/vue-datepicker": "^8.8.1",
    "date-fns": "^3.6.0",
    "openapi-fetch": "^0.9.8",
    "vee-validate": "^4.13.2",
    "vue": "^3.4.35",
    "vue-router": "^4.4.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@playwright/test": "^1.45.3",
    "@redocly/cli": "^1.19.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@storybook/addon-essentials": "^8.2.7",
    "@storybook/addon-interactions": "^8.2.7",
    "@storybook/addon-links": "^8.2.7",
    "@storybook/blocks": "^8.2.7",
    "@storybook/test": "^8.2.7",
    "@storybook/test-runner": "^0.18.2",
    "@storybook/vue3": "^8.2.7",
    "@storybook/vue3-vite": "^8.2.7",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/vue": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.14",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-prefer-arrow-functions": "^3.3.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "happy-dom": "^14.12.3",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "msw": "^2.3.5",
    "msw-storybook-addon": "^2.0.3",
    "npm-run-all2": "^6.2.2",
    "openapi-msw": "^0.6.1",
    "openapi-typescript": "^7.3.0",
    "playwright": "^1.45.3",
    "prettier": "^3.3.3",
    "storybook": "^8.2.7",
    "typescript": "~5.4.5",
    "vite": "^5.3.5",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.29"
  },
  "volta": {
    "node": "20.16.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}