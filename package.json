{
  "name": "vue-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "postbuild": "rm dist/mockServiceWorker.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:screenshot": "SCREEN_SHOT=true test-storybook",
    "test:unit": "vitest",
    "test:story": "test-storybook",
    "test:story2": "vitest --project=storybook",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "format": "prettier --write src/",
    "gen:schema": "openapi-typescript -t ./openapi.yaml -o ./src/shared/api/v1.schema.ts --properties-required-by-default",
    "postgen:schema": "prettier --write ./src/shared/api/v1.schema.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "lint-staged": {
    "*.{vue,ts}": "pnpm format"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.15.1",
    "@vercel/edge": "^1.2.2",
    "@vuepic/vue-datepicker": "^12.0.4",
    "basic-auth": "^2.0.1",
    "date-fns": "^4.1.0",
    "openapi-fetch": "^0.15.0",
    "static-auth": "^2.1.2",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-links": "^10.0.7",
    "@storybook/addon-vitest": "^10.0.7",
    "@storybook/test-runner": "^0.24.1",
    "@storybook/vue3-vite": "^10.0.7",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@tsconfig/node22": "^22.0.3",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "^4.0.8",
    "@vitest/eslint-plugin": "^1.4.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-storybook": "^10.0.7",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-vue": "^10.5.1",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "happy-dom": "^20.0.10",
    "jiti": "^2.6.1",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.6",
    "msw": "^2.12.1",
    "msw-storybook-addon": "^2.0.6",
    "npm-run-all2": "^8.0.4",
    "openapi-msw": "^2.0.0",
    "openapi-typescript": "^7.10.1",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "puppeteer": "^24.29.1",
    "reg-keygen-git-hash-plugin": "^0.14.5",
    "reg-notify-github-plugin": "^0.14.5",
    "reg-suit": "^0.14.5",
    "storybook": "^10.0.7",
    "storycap": "^5.0.1",
    "storycap-testrun": "^1.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.8",
    "vue-tsc": "^3.1.3",
    "@storybook/addon-docs": "^10.0.7"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "msw",
      "puppeteer",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
