{
  "name": "vue-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "postbuild": "rm dist/mockServiceWorker.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:screenshot": "SCREEN_SHOT=true test-storybook",
    "test:unit": "vitest",
    "test:story": "test-storybook",
    "test:story2": "vitest --project=storybook",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "format": "prettier --write src/",
    "gen:schema": "openapi-typescript -t ./openapi.yaml -o ./src/shared/api/v1.schema.ts --properties-required-by-default",
    "postgen:schema": "prettier --write ./src/shared/api/v1.schema.ts",
    "prepare": "husky",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "lint-staged": {
    "*.{vue,ts}": "pnpm format"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.15.0",
    "@vercel/edge": "^1.2.1",
    "@vuepic/vue-datepicker": "^11.0.1",
    "basic-auth": "^2.0.1",
    "date-fns": "^4.1.0",
    "openapi-fetch": "^0.13.4",
    "static-auth": "^2.1.2",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@playwright/test": "^1.50.1",
    "@storybook/addon-essentials": "^8.5.5",
    "@storybook/addon-interactions": "^8.5.5",
    "@storybook/addon-links": "^8.5.5",
    "@storybook/blocks": "^8.5.5",
    "@storybook/test": "^8.5.5",
    "@storybook/test-runner": "^0.22.0",
    "@storybook/vue3": "^8.5.5",
    "@storybook/vue3-vite": "^8.5.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/browser": "^3.0.5",
    "@vitest/eslint-plugin": "^1.1.31",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.20.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "happy-dom": "^17.1.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "msw": "^2.7.0",
    "msw-storybook-addon": "^2.0.4",
    "npm-run-all2": "^7.0.2",
    "openapi-msw": "^1.0.0",
    "openapi-typescript": "^7.6.1",
    "playwright": "^1.50.1",
    "prettier": "^3.5.0",
    "puppeteer": "^24.2.0",
    "reg-keygen-git-hash-plugin": "^0.14.3",
    "reg-notify-github-plugin": "^0.14.4",
    "reg-suit": "^0.14.4",
    "storybook": "^8.5.5",
    "storycap": "^5.0.1",
    "storycap-testrun": "^0.0.2",
    "textlint": "^14.4.2",
    "textlint-rule-preset-ja-technical-writing": "^12.0.2",
    "tsx": "^4.19.2",
    "typescript": "~5.7.3",
    "vite": "^6.1.0",
    "vitepress": "^1.6.3",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.0"
  },
  "volta": {
    "node": "22.11.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "core-js",
      "esbuild",
      "msw",
      "puppeteer"
    ]
  }
}